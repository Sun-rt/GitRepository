/* 提交案例 */

.formrow {
  height: 50px;
  min-height: 50px;
  display: block;
}

.formrow a {
  font-size: 13px;
  font-weight: normal;
  line-height: 22px;
  color: #999999;
}
.formselect{
  height: 30px;
  width: 100%;
}
.forminput {
  height: 26px;
  width: 100%;
  border: 1px solid #aaaaaa;
}

.forminputlabel {
  text-align: right;
}

.formtextarea {
  border:2px solid #eee;
  min-height: 300px;
  width: 100%;

}

.uploadpic{
  margin-bottom: 20px;
}

.imageView img{
   position: absolute;
   top: 0px;
   left: 20px;

	height: 50px;
	width:50px;

}
.deletecolor{
  position: relative;
  top:0px;
  left:13px;
   padding: 0px;
}
.deletecolor a { 
 margin: 0px;
 padding: 0px;
 z-index: 20;
}
.deleteimg{

 position: relative;
 top: -2px;
 left:40px;
padding: 0px;
 margin: 0px;
 z-index: 20;
}
.deleteimg a{
   margin: 0px;
 padding: 0px;

}

.coloritem{
  margin:3px;
